*===========================================================================
* Aging of SOA as in Lane et al., 2008 (Tsimpidi et al., 2010, seem to use 10 times faster aging also for SOA!)
* NOTE! Test mass increase due to uptake of oxygen in aging reactions (assume 7.5% mass increase per aging reaction).
*
4.0e-12*Fgas(ALK4_ug10,:)  ALK4_ug10 + [OH]     =  ALK4_ug1 + 6.12 NON_C_ASOA_ug1 ;
4.0e-12*Fgas(ALK4_ug1,:)   ALK4_ug1 + [OH]      =  ALK4_ng100 + 6.12 NON_C_ASOA_ng100 ;
*
4.0e-12*Fgas(OLE1_ug1,:)   [OH] + OLE1_ug1      = OLE1_ng100 + 4.59 NON_C_ASOA_ng100 ;
4.0e-12*Fgas(OLE1_ug10,:)  [OH] + OLE1_ug10     = OLE1_ug1 + 4.59 NON_C_ASOA_ug1 ;
4.0e-12*Fgas(OLE1_ug1e2,:) [OH] + OLE1_ug1e2    = OLE1_ug10 + 4.59 NON_C_ASOA_ug10 ;
4.0e-12*Fgas(OLE1_ug1e3,:) [OH] + OLE1_ug1e3    = OLE1_ug1e2 + 4.59 NON_C_ASOA_ug1e2 ; 
*
4.0e-12*Fgas(ARO2_ug1,:)   [OH] + ARO2_ug1      = ARO2_ng100 + 15.12 NON_C_ASOA_ng100 ;
4.0e-12*Fgas(ARO2_ug10,:)  [OH] + ARO2_ug10     = ARO2_ug1 + 15.12 NON_C_ASOA_ug1 ;
4.0e-12*Fgas(ARO2_ug1e2,:) [OH] + ARO2_ug1e2    = ARO2_ug10 + 15.12 NON_C_ASOA_ug10 ; 
4.0e-12*Fgas(ARO2_ug1e3,:) [OH] + ARO2_ug1e3    = ARO2_ug1e2 + 15.12 NON_C_ASOA_ug1e2 ;
*
4.0e-12*Fgas(ISOP_ug1,:)   [OH] + ISOP_ug1      = ISOP_ng100 + 9 NON_C_BSOA_ng100 ;
4.0e-12*Fgas(ISOP_ug10,:)  [OH] + ISOP_ug10     = ISOP_ug1 + 9 NON_C_BSOA_ug1  ;
4.0e-12*Fgas(ISOP_ug1e2,:) [OH] + ISOP_ug1e2    = ISOP_ug10 + 9 NON_C_BSOA_ug10 ;
4.0e-12*Fgas(ISOP_ug1e3,:) [OH] + ISOP_ug1e3    = ISOP_ug1e2 + 9 NON_C_BSOA_ug1e2 ;
*
4.0e-12*Fgas(TERP_ug1,:)   [OH] + TERP_ug1      = TERP_ng100 + 15.3 NON_C_BSOA_ng100 ;
4.0e-12*Fgas(TERP_ug10,:)  [OH] + TERP_ug10     = TERP_ug1 + 15.3 NON_C_BSOA_ug1  ;
4.0e-12*Fgas(TERP_ug1e2,:) [OH] + TERP_ug1e2    = TERP_ug10 + 15.3 NON_C_BSOA_ug10 ;
4.0e-12*Fgas(TERP_ug1e3,:) [OH] + TERP_ug1e3    = TERP_ug1e2 + 15.3 NON_C_BSOA_ug1e2 ;
*
4.0e-12*Fgas(NON_C_ASOA_ug1,:)      [OH] + NON_C_ASOA_ug1     =  1.075 NON_C_ASOA_ng100 ;
4.0e-12*Fgas(NON_C_ASOA_ug10,:)     [OH] + NON_C_ASOA_ug10    =  1.075 NON_C_ASOA_ug1  ;
4.0e-12*Fgas(NON_C_ASOA_ug1e2,:)    [OH] + NON_C_ASOA_ug1e2   =  1.075 NON_C_ASOA_ug10  ;
*
4.0e-12*Fgas(NON_C_BSOA_ug1,:)      [OH] + NON_C_BSOA_ug1     =  1.075 NON_C_BSOA_ng100 ;
4.0e-12*Fgas(NON_C_BSOA_ug10,:)     [OH] + NON_C_BSOA_ug10    =  1.075 NON_C_BSOA_ug1  ;
4.0e-12*Fgas(NON_C_BSOA_ug1e2,:)    [OH] + NON_C_BSOA_ug1e2   =  1.075 NON_C_BSOA_ug10  ;
*
