4.0e-11*Fgas(OXFFUEL_ug1e3,:)    [OH] + OXFFUEL_ug1e3   = OXFFUEL_ug1e2 + 0.0756 O_FFUEL_ug1e2 ;
4.0e-11*Fgas(OXFFUEL_ug1e4,:)    [OH] + OXFFUEL_ug1e4   = OXFFUEL_ug1e3 + 0.0756 O_FFUEL_ug1e3 ;
4.0e-11*Fgas(OXFFUEL_ug1e5,:)    [OH] + OXFFUEL_ug1e5   = OXFFUEL_ug1e4 + 0.0756 O_FFUEL_ug1e4 ;

4.0e-11*Fgas(O_FFUEL_ng100,:)    [OH] + O_FFUEL_ng100   =  1.075 O_FFUEL_ng10 ;
4.0e-11*Fgas(O_FFUEL_ug1,:)      [OH] + O_FFUEL_ug1     =  1.075 O_FFUEL_ng100 ;
