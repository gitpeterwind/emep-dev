DJ(IDCH3COX,k)                           RN8OOH = CH3COO2 + HCHO + OH ;
DJ(IDCH3O2H,k)                           RN11OOH = RN10O2 + OH ;
DJ(IDCH3O2H,k)                           RN14OOH = RN13O2 + OH ;

