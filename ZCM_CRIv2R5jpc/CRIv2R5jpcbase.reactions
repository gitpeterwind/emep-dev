DJ(IDCH3O2H,k)                           RN9OOH = CH3CHO + HCHO + HO2 + OH ;
DJ(IDCH3O2H,k)                           RN12OOH = CH3CHO + CH3CHO + HO2 + OH ;
DJ(IDCH3O2H,k)                           RN15OOH = C2H5CHO + CH3CHO + HO2 + OH ;
DJ(IDCH3O2H,k)                           NRN6OOH = HCHO + HCHO + NO2 + OH ;
DJ(IDCH3O2H,k)                           NRN9OOH = CH3CHO + HCHO + NO2 + OH ;
DJ(IDCH3O2H,k)                           NRN12OOH = CH3CHO + CH3CHO + NO2 + OH ;
